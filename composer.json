{
  "name": "WPML/CacheUserQuery",
  "description": "MU-Plugin to cache user query. Useful on sites with big number of users",
  "license": "GPL-2.0-only",
  "keywords": [
    "cache",
    "user query",
    "WP_User_Query"
  ],
  "homepage": "https://github.com/OnTheGoSystems/cache-user-query",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "Igor Gergel",
      "email": "igor.g@onthegosystems.com",
      "homepage": "https://www.onthegosystems.com/"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "5.6"
    }
  },
  "require": {
    "composer/installers": "^1.9"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "antecedent/patchwork": "^2.0",
    "sebastian/phpcpd": "^3.0",
    "10up/wp_mock": "~0.2",
    "phpunit/phpunit": "~5.7",
    "lucatume/function-mocker": "~1.3",
    "squizlabs/php_codesniffer": "^3.0",
    "phpcompatibility/php-compatibility": "^9.0",
    "phpcompatibility/phpcompatibility-wp": "^2.1",
    "wp-coding-standards/wpcs": "^2.0"
  },
  "autoload": {
    "classmap": [
      "classes"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "classes",
      "tests/phpunit/classes"
    ]
  }
}
